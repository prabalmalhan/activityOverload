<div class="containerWithExtraSpace">
	<h3> Users</h3>
	<table class='table'>

	<tr>
	<th>Online</th>
	<th>ID</th>
	<th>Name</th>
	<th>Title</th>
	<th>Email</th>
	<th>Admin</th>
	<th></th>
	<th></th>
	<th></th>
	</tr>


<% _.each(users,function(user){%>

	<tr data-id="<%= user.id %>" data-model="user">
	<td><% if (user.online) {%>
					True
	<% }else{%>
	False

	<% } %>
	<td><%= user.id %></td>
	<td><%= user.name %></td>
	<td><%= user.title %></td>
	<td><%= user.email %></td>
	<td><% if (user.admin) {%>
					True
	<% }else{%>
	False

	<% } %>


	
	</td>
	<td><a href="/user/show/<%= user.id %>" class="btn btn-sm btn-primary">Show</a></td>
	<td><a href="/user/edit/<%= user.id %>" class="btn btn-sm btn-warning">Edit</a></td>
	<td>
	<form action='/user/destroy/<%= user.id %>' method='POST'>
		<input type="hidden" name="method" value="delete"/>
		<input type="submit" class ="btn btn-sm btn-danger" value="Delete"/>
		<input type ="hidden" name="_csrf" value="<$=_csrf %>"/>
	</form></td>

	
	</tr>

	<% }) %>
	</table>
	</div>